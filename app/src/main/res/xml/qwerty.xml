<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="8.3%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row android:keyWidth="7.65%p">
        <Key android:keyLabel="`" android:keyOutputText="~" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="1" android:keyOutputText="!"/>
        <Key android:keyLabel="2" android:keyOutputText="\@"/>
        <Key android:keyLabel="3" android:keyOutputText="\#"/>
        <Key android:keyLabel="4" android:keyOutputText="$"/>
        <Key android:keyLabel="5" android:keyOutputText="%"/>
        <Key android:keyLabel="6" android:keyOutputText="^"/>
        <Key android:keyLabel="7" android:keyOutputText="&amp;"/>
        <Key android:keyLabel="8" android:keyOutputText="*"/>
        <Key android:keyLabel="9" android:keyOutputText="("/>
        <Key android:keyLabel="0" android:keyOutputText=")"/>
        <Key android:keyLabel="-" android:keyOutputText="_"/>
        <Key android:keyLabel="=" android:keyOutputText="+" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:keyLabel="q"/>
        <Key android:keyLabel="w"/>
        <Key android:keyLabel="e"/>
        <Key android:keyLabel="r"/>
        <Key android:keyLabel="t"/>
        <Key android:keyLabel="y"/>
        <Key android:keyLabel="u"/>
        <Key android:keyLabel="i"/>
        <Key android:keyLabel="o"/>
        <Key android:keyLabel="p"/>
        <Key android:keyLabel="[" android:keyOutputText="{"/>
        <Key android:keyLabel="]" android:keyOutputText="}"/>
    </Row>

    <Row>
        <Key android:keyLabel="a"/>
        <Key android:keyLabel="s"/>
        <Key android:keyLabel="d"/>
        <Key android:keyLabel="f"/>
        <Key android:keyLabel="g"/>
        <Key android:keyLabel="h"/>
        <Key android:keyLabel="j"/>
        <Key android:keyLabel="k"/>
        <Key android:keyLabel="l"/>
        <Key android:keyLabel=";" android:keyOutputText=":" android:codes="59" />
        <Key android:keyLabel="'" android:keyOutputText="&quot;" android:codes="39" />
        <Key android:keyLabel="\\" android:keyOutputText="|" android:codes="92" />
    </Row>
    
    <Row>
        <Key android:keyLabel="z"/>
        <Key android:keyLabel="x"/>
        <Key android:keyLabel="c"/>
        <Key android:keyLabel="v"/>
        <Key android:keyLabel="b"/>
        <Key android:keyLabel="n"/>
        <Key android:keyLabel="m"/>
        <Key android:keyLabel="," android:keyOutputText="&lt;"/>
        <Key android:keyLabel="." android:keyOutputText="&gt;"/>
        <Key android:keyLabel="/" android:keyOutputText="\?"/>
        <Key android:keyWidth="16.6%p" android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:isRepeatable="true" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">

        <Key  android:codes="-4" android:keyIcon="@drawable/mic"
                android:keyWidth="10%p" android:keyEdgeFlags="left"/>

        <Key  android:codes="-303" android:keyIcon="@drawable/ic_emoji_people_light_activated"
                android:keyWidth="10%p"/>

        <Key android:codes="-404" android:keyIcon="@drawable/sym_keyboard_lang"
                android:keyWidth="10%p"/>

        <Key android:keyWidth="10%p" android:codes="-21" android:keyLabel="&lt;-" android:isRepeatable="true"/>
        <Key android:codes="32" android:keyLabel="Space"
                android:keyWidth="30%p" android:isRepeatable="true"/>

        <Key android:keyWidth="10%p" android:codes="-22" android:keyLabel="-&gt;" android:isRepeatable="true"/>

        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
                android:keyWidth="20%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
